{
  "name": "scalextric",
  "version": "1.0.1",
  "description": "Like Scala for Web programmers",
  "main": "dist/scalextric.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --mode=development",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint src",
    "test:unit": "nyc ts-mocha -p tsconfig.json test/**/*.spec.ts",
    "test:bundle": "npm run build && mocha test/**/*.spec.js",
    "chords": "python src/utils/chordtable/generate.py > data/chords.json && echo 'Generate data/chords.json file'",
    "scales": "wget -q -O data/scales.zip http://huygens-fokker.org/docs/scales.zip && unzip -o data/scales.zip -d data/ && rm data/scales.zip"
  },
  "author": "Karim Ratib",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-deep-closeto": "^0.1.1",
    "eslint": "^8.2.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "ts-loader": "^9.2.6",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "fraction.js": "^4.1.2"
  }
}
